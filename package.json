    "@walletconnect/modal": "^2.6.2",
    "@walletconnect/sign-client": "^2.15.2",
    "@walletconnect/types": "^2.15.2",
    "add": "^2.0.6",
    "addons-linter": "^6.17.0",
    "concurrently": "^7.6.0",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "dotenv": "^16.3.1",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.6.0",
