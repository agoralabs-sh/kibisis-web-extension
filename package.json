    "prettier": "^2.8.4",
    "semantic-release": "^24.1.0",
    "style-loader": "^3.3.3",
    "thread-loader": "^4.0.2",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^3.14.2",
    "typescript": "4.9.5",
    "web-ext": "^8.2.0",
    "webpack": "^5.94.0",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^5.1.4",
